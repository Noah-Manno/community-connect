<div class="row">
    <div class="col s12 m3 l4 side-image-events">
        <h3 class="white-text">Events</h3>
    </div>
    <div class="col s12 m9 l8 main-events-container">
        {{#if events.length}}
        <div class="bolder-container">
            <h1 class="black-text bold">All Events</h1>
        </div>
        {{else}}
        <h1 class="bold black-text">No Events Scheduled</h1>
        {{/if}}
    </div>
</div>